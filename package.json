{
  "name": "typescript-fastify-starter",
  "version": "1.0.0",
  "description": "Starter for fastify with TypeScript",
  "main": "index.js",
  "scripts": {
    "watch": "concurrently \"npm run type-check:watch\" \"npm run build:js -- --watch\" \"nodemon lib/server.js\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit --resolveJsonModule",
    "type-check:watch": "npm run type-check -- --watch --preserveWatchOutput",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edvardchen/typescript-fastify-starter.git"
  },
  "keywords": [
    "airpay",
    "admin",
    "auth",
    "gateway"
  ],
  "author": "edvardchen",
  "license": "ISC",
  "dependencies": {
    "@types/serve-static": "^1.13.2",
    "fastify": "^2.1.0",
    "fastify-cookie": "^3.0.2",
    "googleapis": "^39.1.0",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/node": "^11.13.0",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "nodemon": "^1.18.10",
    "typescript": "^3.4.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
